{
    "name": "paperbits-demo",
    "version": "0.1.459",
    "description": "An example of using Paperbits to build web apps with rich content editing capability.",
    "author": "Paperbits",
    "license": "MIT",
    "keywords": [
        "paperbits",
        "demo"
    ],
    "scripts": {
        "start": "webpack-dev-server --open --config webpack.develop.js",
        "build": "webpack --config webpack.build.js",
        "build-designer": "webpack --config webpack.designer.js",
        "build-publisher": "webpack --config webpack.publisher.js",
        "build-runtime": "webpack --config webpack.runtime.js",
        "publish": "webpack --config webpack.publisher.js && node dist/publisher/index.js && npm run serve-website",
        "serve-website": "webpack-dev-server --open --contentBase dist/website --quiet"
    },
    "devDependencies": {
        "@types/google.maps": "^3.46.0",
        "@types/knockout.mapping": "^2.0.36",
        "@types/knockout.validation": "0.0.38",
        "@types/mime": "^2.0.3",
        "autoprefixer": "10.4.0",
        "clean-webpack-plugin": "2.0.2",
        "copy-webpack-plugin": "^6.2.1",
        "css-loader": "5.0.0",
        "file-loader": "^6.1.1",
        "html-loader": "^1.3.2",
        "mini-css-extract-plugin": "^1.6.0",
        "path": "^0.12.7",
        "postcss-loader": "^4.0.4",
        "raw-loader": "^4.0.2",
        "sass": "^1.43.4",
        "sass-loader": "10.1.1",
        "style-loader": "2.0.0",
        "terser": "5.9.0",
        "terser-webpack-plugin": "4.1.0",
        "ts-loader": "^8.3.0",
        "typescript": "4.4.4",
        "url-loader": "4.1.1",
        "webpack": "4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "3.11.0",
        "webpack-merge": "^5.1.2"
    },
    "dependencies": {
        "@paperbits/common": "0.1.459",
        "@paperbits/core": "0.1.459",
        "@paperbits/emails": "0.1.459",
        "@paperbits/firebase": "0.1.459",
        "@paperbits/forms": "0.1.459",
        "@paperbits/gtm": "0.1.459",
        "@paperbits/intercom": "0.1.459",
        "@paperbits/prosemirror": "0.1.459",
        "@paperbits/styles": "0.1.459",
        "@webcomponents/custom-elements": "^1.5.0",
        "core-js": "^3.19.0",
        "knockout": "^3.5.1",
        "knockout-mapping": "^2.6.0",
        "knockout.validation": "^2.0.4",
        "mime": "^2.5.2"
    }
}
