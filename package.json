        "clean-webpack-plugin": "2.0.2",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.4.2",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^5.0.2",
        "html-loader": "^0.5.5",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.13.0",
        "path": "^0.12.7",
        "postcss-loader": "^3.0.0",
        "sass-loader": "8.0.1",
        "style-loader": "1.1.2",
        "terser": "4.6.2",
        "terser-webpack-plugin": "2.3.2",
