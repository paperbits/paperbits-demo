{
    "name": "paperbits-demo",
    "version": "0.1.515",
    "description": "An example of using Paperbits to build web apps with rich content editing capability.",
    "author": "Paperbits",
    "license": "MIT",
    "keywords": [
        "paperbits",
        "demo"
    ],
    "scripts": {
        "start": "webpack serve --open --config webpack.develop.js",
        "build": "webpack --config webpack.build.js",
        "build-designer": "webpack --config webpack.designer.js",
        "build-publisher": "webpack --config webpack.publisher.js",
        "build-runtime": "webpack --config webpack.runtime.js",
        "publish": "webpack --config webpack.publisher.js && node dist/publisher/index.js && npm run serve-website",
        "serve-website": "webpack serve --open --static ./dist/website --no-stats",
        "lint": "eslint src/**/*.ts"
    },
    "devDependencies": {
        "@types/google.maps": "^3.49.2",
        "@types/knockout.mapping": "^2.0.37",
        "@types/knockout.validation": "0.0.38",
        "@types/mime": "^2.0.3",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "autoprefixer": "10.4.7",
        "buffer": "^6.0.3",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "6.7.1",
        "eslint": "^8.17.0",
        "file-loader": "^6.2.0",
        "html-loader": "^3.1.0",
        "mini-css-extract-plugin": "^2.6.0",
        "postcss-loader": "^7.0.0",
        "raw-loader": "^4.0.2",
        "sass": "^1.52.3",
        "sass-loader": "13.0.0",
        "stream-browserify": "^3.0.0",
        "style-loader": "3.3.1",
        "terser": "5.14.2",
        "terser-webpack-plugin": "5.3.3",
        "ts-loader": "^9.3.0",
        "typescript": "^4.7.3",
        "url-loader": "4.1.1",
        "webpack": "5.73.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "4.9.2",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@paperbits/common": "0.1.515",
        "@paperbits/core": "0.1.515",
        "@paperbits/emails": "0.1.515",
        "@paperbits/firebase": "0.1.515",
        "@paperbits/forms": "0.1.515",
        "@paperbits/gtm": "0.1.515",
        "@paperbits/intercom": "0.1.515",
        "@paperbits/prosemirror": "0.1.515",
        "@paperbits/styles": "0.1.515",
        "@webcomponents/custom-elements": "^1.5.0",
        "core-js": "^3.22.8",
        "knockout": "^3.5.1"
    }
}
