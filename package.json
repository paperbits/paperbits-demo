        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "3.4.2",
        "file-loader": "^5.1.0",
        "html-loader": "^0.5.5",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.13.1",
        "path": "^0.12.7",
        "postcss-loader": "^3.0.0",
        "sass-loader": "8.0.2",
        "style-loader": "1.1.3",
        "terser": "4.6.10",
