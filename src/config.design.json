{
    "firebase": {
        "apiKey": "$FIREBASE_API",
        "authDomain": "$FIREBASE_DOMAIN",
        "databaseURL": "$FIREBASE_DOMAIN",
        "projectId": "$FIREBASE_PROJECT_ID",
        "storageBucket": "$FIREBASE_STORAGE_BUCKET",
        "databaseRootKey": "/",
        "storageBasePath": "/",
        "auth": {
            "basic": {
                "email": "$FIREBASE_AUTH_EMAIL",
                "password": "$FIREBASE_AUTH_PASSWORD"
            }  
        }
    },
    "features": {
        "seo": true,
        "localization": true,
        "preview": true
    },
    "resources": {
        "iconsFonts": "/data/icon-fonts.json",
        "gridSnippets": "/data/grid-snippets.json",
        "blockSnippets": "/data/block-snippets.json"
    },
    "environment": "design"
}